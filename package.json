    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "express-handlebars": "^3.0.0",
    "express-session": "^1.15.6",
    "knex": "^0.15.2",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "sqlite3": "^4.0.2"
